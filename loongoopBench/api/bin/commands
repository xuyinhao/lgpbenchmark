#!/bin/sh
mkdir="${loongoopfs} -mkdir"
count="${loongoopfs} -count"
test="${loongoopfs} -test"
rm="${loongoopfs} -rm"
rmr="${loongoopfs} -rm -r"
rmdir="${loongoopfs} -rmdir"
rmdirIg="$rmdir --ignore-fail-on-non-empty"
touchz="${loongoopfs} -touchz"
ls="${loongoopfs} -ls"
cp="${loongoopfs} -cp"
df="${loongoopfs} -df"
cat="${loongoopfs} -cat"
atf="${loongoopfs} -appendToFile"
chgrp="${loongoopfs} -chgrp"
chmod="${loongoopfs} -chmod"
chown="${loongoopfs} -chown"
du="${loongoopfs} -du"
put="${loongoopfs} -put"
cfl="${loongoopfs} -copyFromLocal"
ctl="${loongoopfs} -copyToLocal"
get="${loongoopfs} -get"
mfl="${loongoopfs} -moveFromLocal"
mv="${loongoopfs} -mv"
text="${loongoopfs} -text"
tail="${loongoopfs} -tail"
truncate="${loongoopfs} -truncate"
find="${loongoopfs} -find"
stat="${loongoopfs} -stat"
statAll="${loongoopfs} -stat %u|%g|%b|%o|%n|%F"
