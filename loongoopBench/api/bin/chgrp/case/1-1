#!/bin/sh
`$rm $apiPath/chgrp1-1 1>/dev/null 2>&1`
`groupdel chgrp1-1g 2>/dev/null `
`$touchz $apiPath/chgrp1-1 2>/dev/null`
`groupadd chgrp1-1g`
`$chgrp chgrp1-1g $apiPath/chgrp1-1 2>/dev/null`
checkOk $? "chgrp1-1g" "$apiPath/chgrp1-1"
#`$rm $apiPath/chgrp1-1 1>/dev/null 2>&1`
#`groupdel chgrp1-1g`
