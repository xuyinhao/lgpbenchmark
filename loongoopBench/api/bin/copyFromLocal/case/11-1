#!/bin/sh
# -d  255字节长度的 文件名
if [ `only_hdp31` -eq 1 ];then
srcPath=`touchFile "cfl11-1_0012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345"`
distPath="$apiPath/"
`$cfl -d $srcPath $distPath 2>/dev/null`
	checkOk $? "$srcPath" "$distPath/cfl11-1_0012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345"
	return $?
fi
if [ `only_hdp31` -eq 0 ];then
	echo 1 ; return $?
fi
echo 0 

	
