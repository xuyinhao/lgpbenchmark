#!/bin/bash

tableName="hive_partinsertinto_1_5"
str_n1=`genString 20`
str_d1=`genString 200`

str_n2=`genString 15`
str_d2=`genString 150`

createPartitionTb  $tableName
${exec}cmd "insert into table ${tableName} partition (month='1') values(1,'${str_n1}','${str_d1}'),(11,'abbc11','add11');"
[ $? -ne 0 ] && echo 1 && return 1

${exec}cmd "insert into table ${tableName}  partition (month='2') values(2,'${str_n2}','${str_d2}');"
[ $? -ne 0 ] && echo 1 && return 1

if [ `checkTableValue "${tableName}" "${str_n1}" partition 1 ` -ne 0  ];then
	echo 1 ; return 1
fi
if [ `checkTableValue "${tableName}" "${str_d1}" partition 1 ` -ne 0  ];then
    echo 1 ; return 1
fi

if [ `checkTableValue "${tableName}" "${str_n2}" partition 2` -ne 0  ];then
    echo 1 ; return 1
else
	checkTableValue "${tableName}" "${str_d2}" partition 2
fi




