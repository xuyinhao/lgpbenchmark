#!/bin/sh
fileName1="$apiPath/test1-4a"
fileName2="$apiPath/test1-4b"
realPath1=`getLeofsRealPath $fileName1`
realPath2=`getLeofsRealPath $fileName2`
echo "aa" > $realPath1
touch $realPath2
`$test -s $fileName1 2>/dev/null`
ret1=$?
`$test -s $fileName2 2>/dev/null`
ret2=$?
if [ 0 -eq $ret1 ] && [ 1 -eq $ret2 ]; then
 echo 1
else
 echo 0
fi
